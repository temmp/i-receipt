<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="65sp">
	<LinearLayout android:id="@+id/LinearLayout01"
		android:layout_width="fill_parent" android:background="@color/white"
		android:layout_height="30sp">
		<TextView android:id="@+id/TextView01" android:paddingLeft="5sp"
			android:textSize="20sp" android:textColor="@color/black"
			android:layout_height="25sp" android:gravity="center_vertical"
			android:isScrollContainer="true" android:maxLines="1" android:scrollHorizontally="true" android:layout_width="160sp">
		</TextView>
		
		<TextView android:id="@+id/TextView02" android:textSize="20sp"
			android:textColor="@color/black" android:layout_height="25sp" android:layout_width="100sp" android:gravity="right|center_vertical"></TextView>
		<TextView android:id="@+id/TextView05" android:text="$"
			android:gravity="center_vertical" android:textSize="20sp"
			android:textColor="@color/black" android:layout_height="25sp"
			android:layout_width="10sp"></TextView>
		<CheckedTextView xmlns:android="http://schemas.android.com/apk/res/android"
			android:checkMark="?android:attr/textCheckMark"
			android:layout_width="30sp" android:layout_height="35sp"
			android:gravity="top" android:layout_marginTop="-10sp"
			android:layout_marginLeft="3sp" android:id="@+id/text1"/>
	</LinearLayout>
	<LinearLayout android:id="@+id/LinearLayout02"
		android:layout_width="fill_parent" android:background="@color/white"
		android:layout_height="30sp">

		<TextView android:id="@+id/TextView03" android:paddingLeft="5sp"
			android:textColor="@color/SteelBlue" android:layout_height="30sp"
			android:layout_gravity="left|center_vertical" android:textSize="15sp"
			android:layout_width="180sp"></TextView>
		
		<TextView android:id="@+id/TextView04" android:textColor="@color/green" android:textSize="15sp"
			android:layout_height="30sp" android:layout_gravity="center_vertical" android:gravity="right" android:layout_width="80sp" android:layout_marginLeft="10sp"></TextView>

	</LinearLayout>
</LinearLayout>